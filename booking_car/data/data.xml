<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="seq_booking_transaction" model="ir.sequence">
        <field name="name">Booking Transaction</field>
        <field name="code">booking.car.transaction.sequence</field>
        <field name="prefix">BT/</field>
        <field name="padding">1</field>
        <field name="company_id" eval="False"/>
    </record>

    <record id="seq_driver" model="ir.sequence">
        <field name="name">Booking Transaction</field>
        <field name="code">booking.car.driver.sequence</field>
        <field name="prefix">DRIVER/</field>
        <field name="padding">1</field>
        <field name="company_id" eval="False"/>
    </record>

    <data noupdate="1">

        <record id="seq_request_booking" model="ir.sequence">
            <field name="name">Request Booking</field>
            <field name="code">booking.car.request.sequence</field>
            <field name="prefix">BR/</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>
