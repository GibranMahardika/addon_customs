<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data>

        <record id="academic_attendee_view_tree" model="ir.ui.view">
            <field name="name">academic_attendee.view.tree</field>
            <field name="model">academic.attendee</field>
            <field name="arch" type="xml">
                <tree string="">
                    <field name="name" />
                    <field name="session_id" />
                    <field name="attendee_id" />
                </tree>
            </field>
        </record>

        <record id="academic_attendee_view_form" model="ir.ui.view">
            <field name="name">academic_attendee.view.form</field>
            <field name="model">academic.attendee</field>
            <field name="arch" type="xml">
                <form string="">
                    <sheet>
                        <group>
                            <group>
                                <field name="name" />
                            </group>
                            <group>
                                <field name="session_id" />
                                <field name="attendee_id" />
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

    </data>


</odoo>